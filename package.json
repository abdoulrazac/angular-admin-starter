{
  "name": "pos",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4800 -o",
    "start:pwa": "ng build && http-server -o -p 4800 -c-1 dist/angular-admin-starter",
    "start-secure": "ng serve --port=4800 --ssl --ssl-key ./certificate/localhost.key --ssl-cert ./certificate/localhost.crt -o",
    "build": "ng build --configuration production --base-href /pos/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "setup": "npm i ng-apexcharts apexcharts moment ngx-moment ngx-toastr boxicons primeng @ng-select/ng-select @ngx-translate/core @ngx-translate/http-loader @angular/material-moment-adapter",
    "bootstrap": "ng add @ng-bootstrap/ng-bootstrap"
  },
  "private": true,
